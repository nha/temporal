{:paths   ["src" "resources"]
 :deps    {org.clojure/clojure      {:mvn/version "1.11.1"}
           io.temporal/temporal-sdk {:mvn/version "1.11.0"}}
 :aliases {:eastwood {:extra-deps {jonase/eastwood {:mvn/version "1.2.3"}}
                        :main-opts  ["-m" "eastwood.lint" "{:source-paths,[\"src\"]}"]}
           :build {:deps       {io.github.clojure/tools.build {:git/tag "v0.8.1" :git/sha "7d40500"}
                                io.github.seancorfield/build-clj {:git/tag "v0.8.0" :git/sha "9bd8b8a"}}
                   :ns-default build}
           :dev   {:extra-paths ["dev"]
                   :extra-deps  {cider/cider-nrepl            {:mvn/version "0.28.3"}
                                 io.temporal/temporal-testing {:mvn/version "1.11.0"}}
                   :main-opts   ["-m" "nrepl.cmdline" "--middleware" "[cider.nrepl/cider-middleware]"]}
           :test  {:extra-paths ["test"]
                   :extra-deps  {org.clojure/test.check       {:mvn/version "1.1.1"}
                                 lambdaisland/kaocha          {:mvn/version "1.66.1034"}
                                 io.temporal/temporal-testing {:mvn/version "1.11.0"}}
                   :main-opts   ["-m" "kaocha.runner"]}
           :deploy {:extra-deps {slipset/deps-deploy {:mvn/version "RELEASE"}}
                    :exec-fn    deps-deploy.deps-deploy/deploy
                    :exec-args  {;:installer      :remote
                                 :installer :local
                                 ;;:sign-releases? true
                                 :artifact       "deps-deploy.jar"
                                 }}}}
